<resources>
    <!--导航布局-->
    <declare-styleable name="EasyTabBarBottom">
        <attr name="topLineColor" format="color" />
        <attr name="topLineSize" format="dimension" />
        <attr name="bottomLineColor" format="color" />
        <attr name="bottomLineSize" format="dimension" />
        <attr name="itemSize">
            <enum name="one" value="1" />
            <enum name="two" value="2" />
            <enum name="three" value="3" />
            <enum name="four" value="4" />
            <enum name="five" value="5" />
        </attr>
        <attr name="text0" format="string" />
        <attr name="text1" format="string" />
        <attr name="text2" format="string" />
        <attr name="text3" format="string" />
        <attr name="text4" format="string" />
        <attr name="textSelectColor" format="color" />
        <attr name="textUnSelectColor" format="color" />
        <attr name="textSize" format="dimension" />

        <attr name="img0_0" format="reference" />
        <attr name="img0_1" format="reference" />;
        <attr name="img1_0" format="reference" />
        <attr name="img1_1" format="reference" />
        <attr name="img2_0" format="reference" />
        <attr name="img2_1" format="reference" />
        <attr name="img3_0" format="reference" />
        <attr name="img3_1" format="reference" />
        <attr name="img4_0" format="reference" />
        <attr name="img4_1" format="reference" />

        <attr name="android:layout_width" />
        <attr name="android:layout_height" />
    </declare-styleable>

</resources>
