<resources>
    <!--自定义-->
    <declare-styleable name="EasyCustomImage">
        <attr name="type">
            <enum name="none" value="0" /><!--默认-->
            <enum name="circle" value="1" /><!--圆形-->
            <enum name="rounded_rect" value="2" /><!--圆角-->
        </attr>
        <attr name="borderColor" format="color" />
        <attr name="borderWidth" format="dimension" />
        <attr name="rectRoundRadius" format="dimension" />
    </declare-styleable>

