<resources>

    <!--UpDownChooseTextView-->
    <declare-styleable name="UpDownChooseTextView">
        <attr name="upDownTextSizeUp" format="dimension" />
        <attr name="upDownTextColorUp" format="color" />
        <attr name="upDownTextSizeDown" format="dimension" />
        <attr name="upDownTextColorDown" format="color" />
        <attr name="upDownTextColorDownChoose" format="color" />
        <attr name="upDownTextUp" format="string" />
        <attr name="upDownTextDown" format="string" />
        <attr name="upDownTextDownHint" format="string" />
        <attr name="upDownTextUpDownSpace" format="dimension" />
        <attr name="upDownChoose" format="boolean" />
        <attr name="upDownAutoScaleDownText" format="boolean" />
        <attr name="upDownTextGravity">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
            <enum name="center" value="2" />
        </attr>

        <attr name="android:layout_width" />
        <attr name="android:layout_height" />
        <attr name="android:paddingLeft" />
        <attr name="android:paddingRight" />
        <attr name="android:paddingTop" />
        <attr name="android:paddingBottom" />
        <attr name="android:padding" />
    </declare-styleable>
</resources>
