<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/parse"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:text="数据解析测试"
        android:textAllCaps="false"/>
    <Button
        android:id="@+id/get"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:text="GET请求"
        android:textAllCaps="false"/>

    <Button
        android:id="@+id/post"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:text="POST请求"
        android:textAllCaps="false"/>

    <Button
        android:id="@+id/put"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:text="PUT请求"
        android:textAllCaps="false"/>

    <Button
        android:id="@+id/download"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:text="下载请求"
        android:textAllCaps="false"/>

    <Button
        android:id="@+id/upload"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:text="上传请求"
        android:textAllCaps="false"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <Button
            android:id="@+id/HttpReflectManager"
            android:layout_width="match_parent"
            android:layout_height="64dp"
            android:text="解析参数测试(HttpReflectManager)"/>

        <Button
            android:id="@+id/HttpReflectManager_getRequestParams"
            android:layout_width="match_parent"
            android:layout_height="46dp"
            android:layout_marginStart="20dp"
            android:text="getRequestParams"
            android:textAllCaps="false"/>

    </LinearLayout>
</LinearLayout>
